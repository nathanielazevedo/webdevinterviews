{
  "name": "webdevinterviews",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "shared"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@webdevinterviews/frontend",
    "dev:frontend": "npm run dev --workspace=@webdevinterviews/frontend",
    "dev:backend": "npm run dev --workspace=@webdevinterviews/backend",
    "dev:shared": "npm run dev --workspace=@webdevinterviews/shared",
    "build": "npm run build --workspaces",
    "build:frontend": "npm run build --workspace=@webdevinterviews/frontend",
    "build:backend": "npm run build --workspace=@webdevinterviews/backend",
    "build:shared": "npm run build --workspace=@webdevinterviews/shared",
    "lint": "npm run lint --workspaces",
    "test": "npm run test --workspaces",
    "generate-api": "node scripts/generate-api.js",
    "clean": "rm -rf packages/*/dist packages/*/node_modules shared/dist shared/node_modules node_modules"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^8.57.0",
    "openapi-typescript": "^7.9.1",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "2.5.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  }
}
